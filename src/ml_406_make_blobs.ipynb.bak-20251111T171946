    "# Step 13: 군집별 산점도와 중심 표시\n",
    "# 설명: 각 군집 샘플을 서로 다른 마커로 표시하고, 클러스터 중심을 강조합니다.\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "centers = kmeans.cluster_centers_\n",
    "unique_labels = np.unique(cluster_labels)\n",
    "markers = [\"o\", \"s\", \"^\", \"P\", \"D\", \"H\", \"x\"]\n",
    "\n",
    "plt.figure(figsize=(8,6))\n",
    "for label in unique_labels:\n",
    "    # 각 라벨에 해당하는 샘플들만 선택\n",
    "    label_cluster = cluster_df[cluster_df[\"kmeans_label\"] == label]\n",
    "    center_x_y = centers[label]\n",
    "    # 샘플 산점도\n",
    "    plt.scatter(\n",
    "        x=label_cluster[\"ftr1\"],\n",
    "        y=label_cluster[\"ftr2\"],\n",
    "        edgecolor=\"k\",\n",
    "        marker=markers[int(label)],\n",
    "        alpha=0.6,\n",
    "        label=f\"cluster {int(label)}\"\n",
    "    )\n",
    "    # 중심을 흰색 마커로 크게 표시 (테두리 강조)\n",
    "    plt.scatter(\n",
    "        x=center_x_y[0],\n",
    "        y=center_x_y[1],\n",
    "        s=200,\n",
    "        color=\"white\",\n",
    "        alpha=0.9,\n",
    "        edgecolor=\"k\",\n",
    "        marker=markers[int(label)],\n",
    "    )\n",
    "    # 중심에 숫자 마커로 레이블 표시\n",
    "    plt.scatter(\n",
    "        x=center_x_y[0],\n",
    "        y=center_x_y[1],\n",
    "        s=70,\n",
    "        color=\"k\",\n",
    "        edgecolor=\"k\",\n",
    "        marker=\"$%d$\" % label,\n",
    "    )\n",
    "\n",
    "plt.legend()\n",
    "plt.xlabel('ftr1')\n",
    "plt.ylabel('ftr2')\n",
    "plt.title('KMeans clustering with centers')\n",
    "plt.show()"
   ]
